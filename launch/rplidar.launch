<?xml version="1.0" ?>
<launch>
	<node name="rplidarNode"          pkg="rplidar_ros"  type="rplidarNode" output="screen">
		<param name="serial_port"         type="string" value="/dev/ttyS1"/>
		<param name="serial_baudrate"     type="int"    value="230400"/>
		<param name="frame_id"            type="string" value="laser"/>
		<param name="inverted"            type="bool"   value="false"/>
		<param name="angle_compensate"    type="bool"   value="true"/>
		<param name="block_angle_1"       type="int"    value="165"/>
		<param name="block_angle_2"       type="int"    value="45"/>
		<param name="block_angle_3"       type="int"    value="285"/>
		<param name="block_range"         type="int"    value="33"/>
	</node>
</launch>
